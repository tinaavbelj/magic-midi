  "config": {
    "port": 3000
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=$npm_package_config_port webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "serve": "cross-env PORT=$npm_package_config_port nodemon --watch server.js server.js",
    "lint": "eslint --ext .js,.vue --env browser src webpack.config.js",
    "precommit": "npm run lint",
    "postinstall": "npm run build",
    "start": "node server.js"
  },
  "engines": {
    "node": "9.2.0",
    "npm": "5.5.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
