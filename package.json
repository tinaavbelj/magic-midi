    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "express": "^4.16.2",
    "file-loader": "^0.11.2",
    "jsmidgen": "^0.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.19",
    "moment": "^2.19.2",
