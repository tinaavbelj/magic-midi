  "repository": {
    "url": "git+https://github.com/tinaavbelj/magic-midi.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/tinaavbelj/magic-midi/issues"
  },
  "contributors": [
    {
      "name": "Tina Avbelj",
